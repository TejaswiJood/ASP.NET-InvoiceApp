@using Invoicing.DataAccess.Entities;
@model Customer
<div class="mb-3">
    <label class="form-label" asp-for="Name">Name</label>
    <span class="text-danger" asp-validation-for="Name"></span>
    <input class="form-control" type="text" asp-for="Name" />
</div>
<div class="mb-3">
    <label class="form-label" asp-for="Address1">Address1</label>
    <span class="text-danger" asp-validation-for="Address1"></span>
    <input class="form-control" type="text" asp-for="Address1" />
</div>
<div class="mb-3">
    <label class="form-label" asp-for="Address2">Address2</label>
    <span class="text-danger" asp-validation-for="Address2"></span>
    <input class="form-control" type="text" asp-for="Address2" />
</div>
<div class="mb-3">
    <label class="form-label" asp-for="City">City</label>
    <span class="text-danger" asp-validation-for="City"></span>
    <input class="form-control" type="text" asp-for="City" />
</div>
<div class="mb-3">
    <label class="form-label" asp-for="ProvinceOrState">ProvinceOrState</label>
    <span class="text-danger" asp-validation-for="ProvinceOrState"></span>
    <input class="form-control" type="text" asp-for="ProvinceOrState" />
</div>
