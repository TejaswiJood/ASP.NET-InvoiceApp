    <span class="text-danger" asp-validation-for="ZipOrPostalCode"></span>
    <input class="form-control" type="text" asp-for="ZipOrPostalCode" />
</div>
<div class="mb-3">
    <label class="form-label" asp-for="Phone">Phone</label>
    <span class="text-danger" asp-validation-for="Phone"></span>
    <input class="form-control" type="text" asp-for="Phone" />
</div>
<div class="mb-3">
    <label class="form-label" asp-for="ContactFirstName">First Name</label>
    <span class="text-danger" asp-validation-for="ContactFirstName"></span>
    <input class="form-control" type="text" asp-for="ContactFirstName" />
</div>
<div class="mb-3">
    <label class="form-label" asp-for="ContactLastName">Last Name</label>
    <span class="text-danger" asp-validation-for="ContactLastName"></span>
    <input class="form-control" type="text" asp-for="ContactLastName" />
